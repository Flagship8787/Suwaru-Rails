<h1>Listing flags</h1>

<table>
  <tr>
    <th>Content</th>
    <th>Content type</th>
    <th>Flagged by</th>
    <th>Flagged by ip</th>
    <th>Removedat</th>
    <th>Removed by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @flags.each do |flag| %>
  <tr>
    <td><%= flag.content_id %></td>
    <td><%= flag.content_type %></td>
    <td><%= flag.flagged_by_id %></td>
    <td><%= flag.flagged_by_ip %></td>
    <td><%= flag.RemovedAt %></td>
    <td><%= flag.removed_by_id %></td>
    <td><%= link_to 'Show', flag %></td>
    <td><%= link_to 'Edit', edit_flag_path(flag) %></td>
    <td><%= link_to 'Destroy', flag, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Flag', new_flag_path %>
