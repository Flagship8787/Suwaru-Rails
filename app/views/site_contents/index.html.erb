<h1>Listing site_contents</h1>

<table>
  <tr>
    <th>Created by</th>
    <th>Modified by</th>
    <th>Content</th>
    <th>Content type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @site_contents.each do |site_content| %>
  <tr>
    <td><%= site_content.created_by_id %></td>
    <td><%= site_content.modified_by_id %></td>
    <td><%= site_content.content_id %></td>
    <td><%= site_content.content_type %></td>
    <td><%= link_to 'Show', site_content %></td>
    <td><%= link_to 'Edit', edit_site_content_path(site_content) %></td>
    <td><%= link_to 'Destroy', site_content, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Site content', new_site_content_path %>
